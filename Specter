local a = loadstring(game:HttpGet("https://raw.githubusercontent.com/DashaBars/Hack/main/Gui"))()
local b = a:Window("Specter")
local d = game:GetService("CoreGui")
local e = game:GetService("Workspace").House.FuseBox
local f = game:GetService("Workspace").Van.Spawn
local g = game:GetService("Players").LocalPlayer
local h = game:GetService("Workspace").House.Bone
local i = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart

-- Protect GUI function
if not pcall(function() return syn.protect_gui end) then
    syn = {}
    syn.protect_gui = function(j)
        j.Parent = d
    end
end

-- Adjust game settings
game:GetService("Workspace").FallenPartsDestroyHeight = -50000
game:GetService("Lighting").Brightness = 2
game:GetService("Lighting").GlobalShadows = false
game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(128, 128, 128)
game:GetService("Lighting").Ambient = Color3.fromRGB(128, 128, 128)
game:GetService("Lighting").FogEnd = 100000

-- Remove Atmosphere objects from Lighting
for k, l in pairs(game:GetService("Lighting"):GetDescendants()) do
    if l:IsA("Atmosphere") then
        l:Destroy()
    end
end

-- Function to create BillboardGui for orbs
local function createOrbBillboard(o)
    local p = Instance.new("BillboardGui")
    local q = Instance.new("TextLabel")
    p.Adornee = o
    p.Name = "Orbs"
    p.Parent = o
    p.Size = UDim2.new(0, 100, 0, 150)
    p.StudsOffset = Vector3.new(0, 1, 0)
    p.AlwaysOnTop = true
    q.Parent = p
    q.BackgroundTransparency = 1
    q.Position = UDim2.new(0, 0, 0, -50)
    q.Size = UDim2.new(0, 100, 0, 100)
    q.Font = Enum.Font.SourceSansSemibold
    q.TextSize = 14
    q.TextColor3 = Color3.fromRGB(30, 144, 255)
    q.TextStrokeTransparency = 0
    q.TextYAlignment = Enum.TextYAlignment.Bottom
    q.Text = "Orb"
    q.ZIndex = 10

    local function updatePosition()
        local r
        r = game:GetService("RunService").RenderStepped:Connect(function()
            if r and workspace:FindFirstChild(o.Name) and o:FindFirstChild("Orb") then
                local s, t = m:WorldToViewportPoint(o.Orb.Position)
                if t then
                    q.Position = Vector2.new(s.X, s.Y)
                    q.Visible = true
                else
                    q.Visible = false
                end
            else
                if game:GetService("Workspace"):FindFirstChild(o.Name) == nil then
                    r:Disconnect()
                end
                q.Visible = true
            end
        end)
    end

    coroutine.wrap(updatePosition)()
end

-- Create BillboardGui for existing orbs and listen for new ones
local m = workspace.CurrentCamera
for u, o in pairs(workspace:GetChildren()) do
    if o.Name == "Orb" then
        coroutine.wrap(createOrbBillboard)(o)
    end
end

workspace.ChildAdded:Connect(function(o)
    if o.Name == "Orb" then
        coroutine.wrap(createOrbBillboard)(o)
    end
end)

-- Functions to create BillboardGui for EMF parts (emfpart5, emfpart3, emfpart4)
local function createEmfBillboard(w, color, text)
    local p = Instance.new("BillboardGui")
    local q = Instance.new("TextLabel")
    p.Adornee = w
    p.Name = "Emf"
    p.Parent = w
    p.Size = UDim2.new(0, 100, 0, 150)
    p.StudsOffset = Vector3.new(0, 1, 0)
    p.AlwaysOnTop = true
    q.Parent = p
    q.BackgroundTransparency = 1
    q.Position = UDim2.new(0, 0, 0, -50)
    q.Size = UDim2.new(0, 100, 0, 100)
    q.Font = Enum.Font.SourceSansSemibold
    q.TextSize = 14
    q.TextColor3 = color
    q.TextStrokeTransparency = 0
    q.TextYAlignment = Enum.TextYAlignment.Bottom
    q.Text = text
    q.ZIndex = 10

    local function updatePosition()
        local x
        x = game:GetService("RunService").RenderStepped:Connect(function()
            if w and workspace:FindFirstChild(w.Name) and w:FindFirstChild(text) then
                local s, t = m:WorldToViewportPoint(w[text].Position)
                if t then
                    q.Position = Vector2.new(s.X, s.Y)
                    q.Visible = true
                else
                    q.Visible = false
                end
            else
                if game:GetService("Workspace"):FindFirstChild(w.Name) == nil then
                    x:Disconnect()
                end
                q.Visible = true
            end
        end)
    end

    coroutine.wrap(updatePosition)()
end

-- Create BillboardGui for emfpart5
for y, w in pairs(workspace:GetChildren()) do
    if w.Name == "emfpart5" then
        coroutine.wrap(createEmfBillboard)(w, Color3.fromRGB(255, 0, 0), "emfpart5")
    end
end

workspace.ChildAdded:Connect(function(w)
    if w.Name == "emfpart5" then
        coroutine.wrap(createEmfBillboard)(w, Color3.fromRGB(255, 0, 0), "emfpart5")
    end
end)

-- Create BillboardGui for emfpart3
for B, z in pairs(workspace:GetChildren()) do
    if z.Name == "emfpart3" then
        coroutine.wrap(createEmfBillboard)(z, Color3.fromRGB(255, 140, 0), "emfpart3")
    end
end

workspace.ChildAdded:Connect(function(z)
    if z.Name == "emfpart3" then
        coroutine.wrap(createEmfBillboard)(z, Color3.fromRGB(255, 140, 0), "emfpart3")
    end
end)

-- Create BillboardGui for emfpart4
for l, C in pairs(workspace:GetChildren()) do
    if C.Name == "emfpart4" then
        coroutine.wrap(createEmfBillboard)(C, Color3.fromRGB(255, 69, 0), "emfpart4")
    end
end

workspace.ChildAdded:Connect(function(C)
    if C.Name == "emfpart4" then
        coroutine.wrap(createEmfBillboard)(C, Color3.fromRGB(255, 69, 0), "emfpart4")
    end
end)

-- ESP for GhostModel
for D, E in pairs(workspace:GetChildren()) do
    if E.Name == "GhostModel" and E:FindFirstChild("Humanoid") then
        local F = Instance.new("BoxHandleAdornment")
        local p = Instance.new("BillboardGui")
        local q = Instance.new("TextLabel")
        F.Adornee = E.HumanoidRootPart
        F.AlwaysOnTop = true
        F.Color3 = Color3.fromRGB(255, 250, 250)
        F.Size = Vector3.new(2, 2, 1)
        F.Transparency = 0.5
        F.ZIndex = 2
        F.Parent = E
        F.Name = "ESP"
        p.Adornee = E.HumanoidRootPart
        p.Name = "Ghost"
        p.Parent = E
        p.Size = UDim2.new(0, 100, 0, 150)
        p.StudsOffset = Vector3.new(0, 1, 0)
        p.AlwaysOnTop = true
        q.Parent = p
        q.BackgroundTransparency = 1
        q.Position = UDim2.new(0, 0, 0, -50)
        q.Size = UDim2.new(0, 100, 0, 100)
        q.Font = Enum.Font.SourceSansSemibold
        q.TextSize = 14
        q.TextColor3 = Color3.new(1, 1, 1)
        q.TextStrokeTransparency = 0
        q.TextYAlignment = Enum.TextYAlignment.Bottom
        q.Text = "Ghost Name: " .. workspace:FindFirstChild("Van").Objectives.SurfaceGui.Frame.Objectives.GhostInfo.Text:split("is ")[2]:match(">(.+)<")
        q.ZIndex = 10
    end
end

